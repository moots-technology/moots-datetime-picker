{
  "extends": "tslint-ionic-rules/strict",
  "rules": {
    "deprecation": {
      "severity": "warning"
    },
    "linebreak-style": false,
    "no-console": {
      "severity": "warning"
    },
    "trailing-comma": false,
    "no-floating-promises": false,
    "strict-boolean-conditions": false
  }
}
